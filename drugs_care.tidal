setcps (120/60/4)



-- VOICE

d1 $ s "in1" # gain (1.5 - (cF 0 "9")*0.90 - (cF 0 "8")*0.90 ) # verb 0.5 0.5 0.1 0.3

d2 $ s "in1" # crush 10 # verb 0.5 0.5 0.9 0.7 # gain ((cF 0 "8")*1.5) # distort 0.1

d3 $ s "in1" # verb 0.5 0.5 0.1 0.8 # delay 2 # delayt 2 # delayfb 0.5 # gain ((cF 0 "9")*1.3)


-- BEAT

xfadeIn "heartbeat" 8 $ slow 2 $ n "[3*2] ~ ~ ~ ~" # sound "909" # speed 0.5 # lpf 800 # verb 0.9 0.8 0.5 0.5 # gain 1.2

xfadeIn "heartbeat" 8 $ silence

hush


-- RECORD

once $ s "looper" # lname "s1"

once $ s "looper" # lname "s2"

once $ s "looper" # lname "s3"

once $ s "looper" # lname "s4"

p "loop" $ s "looper" # lname "c1"

hush

--  PLAY

d4 $ degradeBy 0.5 $ every 2 (brak) $ jux rev $ s "s1!16" # gain 0.8 # legato 1.5 # verb 0.5 0.5 0.9 0.7 # grain "0 0.4 0.8" 0.2 # lpf (range 850 1350 sine) # resonance 0.3 # vowel "<a u i o>" # coarse 128

d5 $ ply "<8 16 32>" $ chop 8 $ s "s2(5,8)" # grain "0 <0.2 0.5>" 0.4 # gain 1.3 # lpf (range 650 900 sine) # verb 0.5 0.5 0.9 0.7

xfadeIn 4 8 $ silence

d5 $ every 2 (degrade) $ chop 16 $ s "s2*4" # timescale 1.3 # timescalewin 0.2 # legato 1 # begin "<0 0.3 0.5>" # octave "<4,5>" # distort 0.1 # coarse 8 # gain 1.1

d6 $ chop 16 $ s "<s3 s4>!4" # octave "<3 4>" # legato 1 # gain 1.4 # verb 0.5 0.5 0.9 0.7 # vowel (choose (["a","u","o"]))

xfadeIn 7 8 $ silence

d6 $ chop 16 $ s "s3!<4 8>" # gain 1.4 # verb 0.5 0.5 0.9 0.7 # legato 1 # lpf (range 750 1200 sine) # resonance 0.2 # pan (slow 2 sine) # octave "<3 4>" # note (scale "minor" "1 1 2 1 2")

d7 $ s "c1!8" # octersub 1 # verb 0.5 0.5 0.9 0.7 # legato 1 # lpf 800 # gain 1.2 # vowel "a" # mu 2

d7 $ s "s4!16" # grain (choose ([0.1,0.3,0.5])) 0.1 # gain 1.2 # verb 0.5 0.5 0.9 0.7

d8 $


hush
